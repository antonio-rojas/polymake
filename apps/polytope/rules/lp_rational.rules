object Polytope<Rational> {

CREDIT cddlib

rule cdd.simplex: LP.MAXIMAL_VALUE, LP.MAXIMAL_VERTEX, FEASIBLE : LP.LINEAR_OBJECTIVE, FACETS | INEQUALITIES {
   cdd_solve_lp($this, $this->LP, 1);
}
weight 3.10;

rule cdd.simplex: LP.MINIMAL_VALUE, LP.MINIMAL_VERTEX, FEASIBLE : LP.LINEAR_OBJECTIVE, FACETS | INEQUALITIES {
   cdd_solve_lp($this, $this->LP, 0);
}
weight 3.10;

CREDIT lrslib

rule lrs.simplex: LP.MAXIMAL_VALUE, LP.MAXIMAL_VERTEX, FEASIBLE, POINTED, LINEALITY_DIM : LP.LINEAR_OBJECTIVE, FACETS | INEQUALITIES {
   lrs_solve_lp($this, $this->LP, 1);
}
weight 3.20;

rule lrs.simplex: LP.MINIMAL_VALUE, LP.MINIMAL_VERTEX, FEASIBLE, POINTED, LINEALITY_DIM : LP.LINEAR_OBJECTIVE, FACETS | INEQUALITIES {
   lrs_solve_lp($this, $this->LP, 0);
}
weight 3.20;

}


