# @category Set Types
declare property_type boost_dynamic_bitset : Set<Int> : c++ ( include => [ "polymake/common/boost_dynamic_bitset.h" ], operators => '+= -= ^=') {

   method contains {
      my ($self, $key)=@_;
      exists $self->{$key};
   }

   # The cardinality of the set.
   # @return Int
   user_method size() : c++;
}


