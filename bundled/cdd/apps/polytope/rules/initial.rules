function H_input_is_feasible(Polytope<Rational>) {
   cdd_input_feasible($_[0]);
}

function H_input_is_feasible(Polytope<Float>) {
   cdd_input_feasible($_[0]);
}

function H_input_is_bounded(Polytope<Rational>) {
   cdd_input_bounded($_[0]);
}

function H_input_is_bounded(Polytope<Float>) {
   cdd_input_bounded($_[0]);
}

